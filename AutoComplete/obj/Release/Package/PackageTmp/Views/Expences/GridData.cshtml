@model IEnumerable<AutoComplete.Models.Expences>

@foreach (var item in Model) {
    <tr data-pkey="@item.ID">
        <td>
            @Html.DisplayFor(modelItem => item.EDate)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        <td class="rowControl hidden">
            <a href="#makeRowEditable" class="makeRowEditable k-button">Edit</a> |
            <a href="#deleteRow" class="deleteRow k-button">Delete</a>
        </td>
    </tr>
}

